<tr>
  <td><%= link_to test.title, test_path(test) %></td>
  <td><%= test_level(test) %></td>
  <td><%= test.questions.count %></td>
  <td class="text-right"><%= link_to 'Edit', edit_test_path(test) %></td>
  <td class="text-right"><%= link_to 'Delete', test_path(test), method: :delete, data: { confirm: 'Are you sure?' } %></td>
  <td class="text-right"><%= button_to 'Start', start_test_path(test), class: 'btn btn-primary btn-sm' %></td>
</tr>
